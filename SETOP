E = {0, 2, 4, 6, 8};
N = {1, 2, 3, 4, 5};
 
# for set union
print("Union of E and N is",E | N)
 
# for set intersection
print("Intersection of E and N is",E & N)
 
# for set difference
print("Difference of E and N is",E - N)
 
# for set symmetric difference
print("Symmetric difference of E and N is",E ^ N)
